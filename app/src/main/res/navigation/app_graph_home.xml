<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" android:id="@+id/app_graph.xml"
    app:startDestination="@id/homeFragment">

    <fragment
        android:id="@+id/homeFragment"
        android:name="com.zistus.multimodule.ui.home.HomeFragment"
        android:label="HomeFragment" >
        <action
            android:id="@+id/action_homeFragment_to_subHomeFragment"
            app:destination="@id/subHomeFragment" />
        <action
            android:id="@+id/action_homeFragment_to_newDocFragment"
            app:destination="@id/newDocFragment" />
    </fragment>
    <fragment
        android:id="@+id/subHomeFragment"
        android:name="com.zistus.multimodule.ui.home.SubHomeFragment"
        android:label="SubHomeFragment" />
    <fragment
        android:id="@+id/newDocFragment"
        android:name="com.zistus.multimodule.ui.newdoc.NewDocFragment"
        android:label="NewDocFragment" />
</navigation>